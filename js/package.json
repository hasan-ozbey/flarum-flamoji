{
    "name": "@the-turk/flarum-flamoji",
    "description": "Simple emoji manager for Flarum.",
    "license": "MIT",
    "version": "0.0.0",
    "dependencies": {
        "@joeattardi/emoji-button": "^4.6.0",
        "@roderickhsiao/emoji-button-locale-data": "^0.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "file-saver": "2.0.5",
        "flarum-webpack-config": "2.0.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "devDependencies": {
        "prettier": "^2.8.3",
        "flarum-webpack-config": "^2.0.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "@flarum/prettier-config": "^1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/the-turk/flarum-flamoji.git"
    },
    "scripts": {
        "dev": "webpack --mode development --watch",
        "build": "webpack --mode production",
        "build-dev": "webpack --mode development --config webpack.config.js",
        "format": "prettier --write src",
        "format-check": "prettier --check src",
        "analyze": "cross-env ANALYZER=true npm run build"
    },
    "private": true,
    "prettier": "@flarum/prettier-config"
